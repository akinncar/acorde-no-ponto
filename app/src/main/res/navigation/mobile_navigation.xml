<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="br.com.acordenoponto.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_new_destiny"
        android:name="br.com.acordenoponto.ui.new_destiny.NewDestinyFragment"
        android:label="@string/title_new_destiny"
        tools:layout="@layout/fragment_new_destiny" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="br.com.acordenoponto.ui.notifications.NotificationsFragment"
        android:label="@string/title_alarm"
        tools:layout="@layout/fragment_notifications" />
</navigation>